import java.util.Scanner;

public class CountofDigits
{
    public static void main(String args[])
    {
        int num, count=0;
        Scanner scan = new Scanner(System.in);
		
        System.out.print("Enter a Number : ");
        num = scan.nextInt();
	while(num!=0)
        {
         num = num/10;
         count++;
        }
		
        System.out.print("Count of Digits is: " +count);        
    }
}
